{
  "AWSEBDockerrunVersion" :"2",
  "authentication": {
    "bucket" : "elasticbeanstalk-us-east-1-163603999016",
    "key": ".dockercfg"
  },
  "containerDefinitions" : [
    {
      "name":"web",
            "image" : "jaicob/web-app:staging-0.6.30",
      "essential": true,
      "memory": 1536,
      "portMappings" : [
        {
          "hostPort": 80,
          "containerPort": 80
        },
        {
          "hostPort":443,
          "containerPort":443
        }
      ],
      "mountPoints":[
        {
          "sourceVolume" :"awseb-logs-web",
          "containerPath":"/home/rails/my-app/log"
        }
      ]
    }
  ]
}
