{
	"AWSEBDockerrunVersion" :"2",
	"authentication": {
		"bucket" : "elasticbeanstalk-us-east-1-163603999016",
		"key": ".dockercfg"
	},
	"containerDefinitions" : [
		{
			"name":"sidekiq",
			"image" : "jaicob/mailer",
			"essential": true,
			"memory": 512
		},
		{
			"name":"web",
			"image" : "jaicob/web-app",
			"essential": true,
			"memory": 1024,
			"portMappings" : [
				{
					"hostPort": 80,
					"containerPort": 80
				},
				{
					"hostPort":443,
					"containerPort":443
				}
			],
			"links" : [
				"sidekiq"
			],
			"environment": [
       			{
        			"name": "REDIS_PRODUCTION_ADDR",
          			"value": "axon-prod.he9z8n.0001.use1.cache.amazonaws.com"
        		},
        		{
        			"name": "REDIS_PRODUCTION_PORT",
          			"value": "6379"
        		}
     		],
			"mountPoints":[
				{
					"sourceVolume" :"awseb-logs-web",
					"containerPath":"/home/rails/my-app/log"
				}
			]
		}
	]
}