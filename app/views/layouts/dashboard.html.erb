<% if current_user.campus_manager || current_user.admin %>
<!-- Displays ADMIN NAVBAR for campus_managers and admin -->
  <% content_for (:admin_nav) do %>
    <%= render "layouts/partials/admin/admin_panel" %>
  <% end %>
<% else %>
<!-- Displays NAVBAR for tutors and students -->
  <% content_for (:dashboard_nav) do %>
    <%= generate_dashboard_nav_links and render 'layouts/partials/dashboard_nav' %>
  <% end %>
<% end %>

<%= render template: "layouts/application" %>