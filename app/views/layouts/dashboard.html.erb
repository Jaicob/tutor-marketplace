<% content_for (:dashboard_nav) do %>
  <%= generate_dashboard_nav_links and render 'layouts/partials/dashboard_nav' %>
<% end %>

<% if current_user.tutor && !@tutor.onboarding_complete? %>
  <% content_for (:new_tutor_checklist) do %>
    <%= render 'layouts/partials/tutors/new_tutor_main' %>
  <% end %>
<% end %>

<% content_for (:flash_messages) do %>
  <div class="row">
    <div class="small-12 columns">
      <p class="flash-message">
        <span class="notice"><%= notice %></span>
        <span class="alert"><%= alert %></span>
      </p>
    </div>
  </div>
<% end %>

<%= render template: "layouts/application" %>