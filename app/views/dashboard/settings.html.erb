<%= form_for :settings_data, url: "/tutors/#{@tutor.id}/settings" do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name, value: @user.first_name %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name, value: @user.last_name %>
  <%= f.label :email %>
  <%= f.text_field :email, value: @user.email %>
  <%= f.label :birthdate %>
  <%= f.date_select :birthdate, value: @tutor.birthdate %>
  <%= f.label :phone_number %>
  <%= f.text_field :phone_number, value: @tutor.phone_number %>
  <%= f.submit 'Submit' %>
<% end %>
User: <%= @user.to_json %>
Tutor: <%= @tutor.to_json %>
