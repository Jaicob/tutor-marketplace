<%= form_for :settings_data, url: "/tutors/#{@tutor.id}/settings" do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, value: @settings_name %>
  <%= f.label :email %>
  <%= f.text_field :email, value: @settings_email %>
  <%= f.label :birthdate %>
  <%= f.date_select :birthdate, value: @settings_birthdate %>
  <%= f.label :phone_number %>
  <%= f.text_field :phone_number, value: @settings_phone_number %>
  <%= f.submit 'Submit' %>
<% end %>
User: <%= @user.to_json %>
Tutor: <%= @tutor.to_json %>