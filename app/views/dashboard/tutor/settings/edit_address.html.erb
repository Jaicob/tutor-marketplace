<section class="dashboard tutor">
  <div class="wrapper">
    <div class="wrapper__text">
      <h2 class="title">Setttings Manager</h2>
    </div>
    <div class="g g-3up">
      <div class="gi">
        <%= render 'vertical_sub_menu' %>
      </div>
      <div class="gi">
        <%= form_for @tutor do |f| %>
          
          <%= f.label :line1, 'Address Line 1' %>
          <%= f.text_field :line1 %>

          <%= f.label :line2, 'Address Line 2' %>
          <%= f.text_field :line2 %>

          <%= f.label :city %>
          <%= f.text_field :city %>

          <%= f.label :state %>
          <%= f.select :state, us_states %>

          <%= f.label :postal_code %>
          <%= f.text_field :postal_code %>

          <%= f.submit 'Update Address', class: 'button success tiny expand' %>

        <% end %>
      </div>
      <div class="gi"></div>
    </div>
  </div>
</section>
